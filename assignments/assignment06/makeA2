Files.AddSearchPath . ~(*------------------ Compilation ----------------------*)WMDefaultFont.Load ~ Compiler.Compile --platform=Bios32 --destPath=Build/	src/I386.Builtins.Mod 	src/Trace.Mod 	src/BIOS.I386.Machine.Mod 	src/Heaps.Mod 	src/Modules.Mod 	src/BIOS.Objects.Mod 	src/Kernel.Mod 	src/KernelLog.Mod 	src/Plugins.Mod 	src/RealConversions.Mod	src/Streams.Mod 	src/Commands.Mod 	src/I386.Reals.Mod 	src/Reflection.Mod 	src/TrapWriters.Mod	src/BIOS.I386.Traps.Mod 	src/BIOS.PCI.Mod 	src/BIOS.Clock.Mod 	src/BIOS.MemCache.Mod 	src/Disks.Mod 	src/Files.Mod  	src/BIOS.ATADisks.Mod 	src/Caches.Mod 	src/DiskVolumes.Mod 	src/DiskFS.Mod 	src/Dates.Mod 	src/Strings.Mod 	src/Caches.Mod 	src/OberonFS.Mod 	src/FATVolumes.Mod 	src/ISO9660Volumes.Mod 	src/BitSets.Mod 	src/Diagnostics.Mod 	src/StringPool.Mod 	src/ObjectFile.Mod 	src/GenericLinker.Mod 	src/Loader.Mod 	src/BootConsole.Mod 	src/Test.Mod~(* ----------------- Linking ---------------- *)Linker.Link -p=Bios32	--fileName="IDE.Bin"	--displacement=0100000H	--path="Build/"	--logFileName="linker.log"	Builtins Trace Machine Heaps Modules Objects Kernel 	KernelLog Plugins Streams Commands Reals 	Reflection TrapWriters Traps PCI Clock MemCache Disks Files 	ATADisks DiskVolumes DiskFS	Dates Strings	Diagnostics BitSets StringPool ObjectFile GenericLinker  Loader 	BootConsole Test	~(* ---------- Create bootable HDD Image ------ *)	System.Timer start ~PCAAMD64.Assemble src/OBLUnreal.Asm ~PartitionsLib.SetBootLoaderFile src/OBLUnreal.Bin ~ FSTools.DeleteFiles -i A2HDD.img ~VirtualDisks.Create A2HDD.img 409248 512 ~VirtualDisks.Install VDISK0 A2HDD.img ~Partitions.WriteMBR VDISK0#0 src/OBEMBR.Bin ~ Partitions.Create VDISK0#1 76 9999 ~Partitions.Activate VDISK0#1 ~ Partitions.Format VDISK0#1 AosFS 1024 IDE.Bin ~FSTools.Mount TEMP AosFS VDISK0#1 ~FSTools.CopyFiles -oq Build/*.GofG => TEMP:*.Gof ~FSTools.CopyFiles -oq Build/*.SymG => TEMP:*.Sym ~FSTools.Watch TEMP ~FSTools.Unmount TEMP ~Partitions.SetConfig VDISK0#1	TraceMode="7" TracePort="1" TraceBPS="115200"	BootVol1="AOS AosFS IDE0#1"	AosFS="DiskVolumes.New DiskFS.NewFS"	Boot1="Test.Test"	Init=""	ATADetect="legacy"	ExtMemSize="256"	CacheSize="1000"	~VirtualDisks.Uninstall VDISK0 ~System.Show HDD image build time:  ~ System.Timer elapsed ~FSTools.CloseFiles A2HDD.img ~